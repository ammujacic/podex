{
  "name": "podex",
  "version": "0.1.0",
  "private": true,
  "description": "Podex - Web-based agentic IDE platform",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "19",
        "react-dom": "19"
      },
      "allowAny": [
        "monaco-editor"
      ]
    },
    "onlyBuiltDependencies": [
      "@sentry/cli",
      "better-sqlite3",
      "esbuild",
      "sharp"
    ],
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@^25.0.0",
      "glob": "^10.4.5",
      "rimraf": "^5.0.5",
      "tar": "^7.4.3",
      "source-map": "^0.7.4",
      "cacache": "^20.0.3",
      "are-we-there-yet": "^4.0.2",
      "boolean": "npm:extra-boolean@^1.11.1",
      "lodash.isequal": "npm:fast-is-equal@^1.2.3",
      "@types/react": "19.2.8",
      "@types/react-dom": "19.2.3"
    }
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "test:coverage": "turbo test:coverage",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prettier": "^3.2.0",
    "turbo": "^2.7.4",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  }
}
